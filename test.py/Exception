class InvalidVoter(Exception):

    def __init__(self,msg):
        self.msg=msg

age=int(input('please enter the age'))

try:
    if age>18:
        print('valid voter')

    else:
        raise InvalidVoter('age is less than 18')

except InvalidVoter:
    print('invalid voter')

print('i am done')




